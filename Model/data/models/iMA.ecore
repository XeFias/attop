<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="iMA" nsURI="iMA" nsPrefix="iMA">
  <eClassifiers xsi:type="ecore:EClass" name="InterimAutomaton">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//State" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="State">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoing" eType="#//Disruption"
        containment="true" eOpposite="#//Disruption/source"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="incoming" upperBound="-1"
        eType="#//Disruption" eOpposite="#//Disruption/target"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bes" upperBound="-1" eType="#//StringToStatusTypeMap"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="gates" upperBound="-1"
        eType="#//StringToStatusTypeMap" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Disruption">
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//State"
        eOpposite="#//State/outgoing"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//State"
        eOpposite="#//State/incoming"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StringToStatusTypeMap" instanceClassName="java.util.Map$Entry">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="#//StatusType"
        defaultValueLiteral="OPERATIONAL"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="key" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral="&quot;&quot;"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="StatusType">
    <eLiterals name="OPERATIONAL" literal="OPERATIONAL"/>
    <eLiterals name="SUCCESS" value="1" literal="SUCCESS"/>
    <eLiterals name="FAILED" value="2"/>
    <eLiterals name="SAFE" value="3"/>
    <eLiterals name="DONTCARE" value="4"/>
  </eClassifiers>
</ecore:EPackage>
